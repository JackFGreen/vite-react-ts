{
  "name": "vite-react-ts",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "prettier": "prettier 'src/**/*.{ts,tsx,vue,htm,html,css,sss,less,scss,sass}' --write",
    "lint:script": "eslint 'src/**/*.{ts,tsx}'",
    "fix:script": "eslint 'src/**/*.{ts,tsx}' --fix",
    "lint:style": "stylelint 'src/**/*.{vue,htm,html,css,sss,less,scss,sass}'",
    "fix:style": "stylelint 'src/**/*.{vue,htm,html,css,sss,less,scss,sass}' --fix",
    "lint": "npm run lint:style && npm run lint:script",
    "fix": "npm run fix:style && npm run fix:script",
    "prepare": "husky install"
  },
  "dependencies": {
    "@jackgreen/react-router-view": "^1.2.1",
    "antd": "^4.10.0",
    "fetch-mock": "^9.11.0",
    "lodash": "^4.17.20",
    "mockjs": "^1.1.0",
    "qs": "^6.9.4",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-router-dom": "^5.2.0",
    "whatwg-fetch": "^3.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.16",
    "@babel/preset-env": "^7.13.15",
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-angular": "^12.1.1",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-eslint": "^8.0.1",
    "@rollup/plugin-typescript": "^8.2.1",
    "@types/lodash": "^4.14.167",
    "@types/mockjs": "^1.0.3",
    "@types/node": "^15.0.1",
    "@types/qs": "^6.9.5",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "@vitejs/plugin-react-refresh": "^1.3.3",
    "commitizen": "^4.2.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^6.0.0",
    "less": "^4.1.1",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "stylelint": "^13.13.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-prettier": "^1.2.0",
    "typescript": "^4.2.4",
    "vite": "^2.2.3",
    "vite-plugin-imp": "^2.0.7"
  }
